<script type="text/javascript">
$(document).ready(function() {
  let allVehicles = <%-JSON.stringify(vehicles)%>;
  let allCivilians = <%-JSON.stringify(civilians)%>;
  if ( allVehicles != null) {
    $('#plateDetails').show()
    $('#plateDatabaseModal').modal('toggle');
    searchPlate()
  } else if (allCivilians != null) {
    $('#civDetails').show()
    $('#nameDatabaseModal').modal('toggle');
    searchNames()
  }
  else {
    $('#plateDetails').removeAttr("style").hide()
    $('#civDetails').removeAttr("style").hide()
  }

  $("#civ-first-name").keydown(function(event) {
    if (event.keyCode == 32) {
        event.preventDefault();
        $('#civ-first-name').popover('show')
    } else {
      $('#civ-first-name').popover('hide')
    }
  });

  $("#civ-last-name").keydown(function(event) {
      if (event.keyCode == 32) {
          event.preventDefault();
          $('#civ-last-name').popover('show')
      } else {
        $('#civ-last-name').popover('hide')
      }
  });

  $("#ticket-civ-first-name").keydown(function(event) {
    if (event.keyCode == 32) {
        event.preventDefault();
        $('#ticket-civ-first-name').popover('show')
    } else {
      $('#ticket-civ-first-name').popover('hide')
    }
  });
  $("#ticket-civ-last-name").keydown(function(event) {
      if (event.keyCode == 32) {
          event.preventDefault();
          $('#ticket-civ-last-name').popover('show')
      } else {
        $('#ticket-civ-last-name').popover('hide')
      }
  });
});

document.addEventListener("DOMContentLoaded", function(){
  tail.select(".select-move", {
    search: true,
    descriptions: true,
    hideSelected: true,
    hideDisabled: true,
    multiShowCount: true,
    multiContainer: true
  });
});

function searchPlate() {
  let x = document.getElementById('plateDetails');
  if (x.style.display === 'none') {
    x.style.display = 'block';
  } else {}

  let myValue = $('#plateName').val();
  let myVar = <%-JSON.stringify(vehicles)%>;
  if (myVar === null) {
    return
  }
  if (myVar.length === 0) {
    $('#plateDetails').removeAttr("style").hide()
    $('#vehicleNotFound').show()
    return
  }
}

  function searchNames() {
    let x = document.getElementById('civDetails');
    if (x.style.display === 'none') {
      x.style.display = 'block';
    } else {}
    let myCiv = <%-JSON.stringify(civilians)%>;
    let myTicket = <%-JSON.stringify(tickets)%>;
    if (myCiv === null || myTicket === null) {
      hideCivPopover();
      return
    } else if (myCiv.length === 0) {
      let civDetails = document.getElementById('civDetails')
      civDetails.style.display = 'none'
      let civNotFoundMsg = document.getElementById('civilianNotFound')
      civNotFoundMsg.style.display = 'block'
      hideCivPopover();
      return
    }
    hideCivPopover();
  }

  // prevents submit modal when pressing enter because it closes the entire window and does not submit.
  // we should fix the modals so this can work as expected.
  $(document).on("keypress", "form", function (event) {
    return event.keyCode != 13;
  });

  function setDateAndTime() {
    document.getElementById('date').valueAsDate = new Date();
    time = document.getElementById('time');
    time.value = new Date().toLocaleTimeString('en-GB', {
      hour: "2-digit",
      minute: "2-digit"
    });
  }

  function setDateAndTimeForWarning() {
    document.getElementById('warning-date').valueAsDate = new Date();
    time = document.getElementById('warning-time');
    time.value = new Date().toLocaleTimeString('en-GB', {
      hour: "2-digit",
      minute: "2-digit"
    });
  }

  $('#ticketModal').on('hidden.bs.modal', function () {
    $(this).find('form').trigger('reset');
  })

  $('#plateDatabaseModal').on('hidden.bs.modal', function () {
    $(this).find('form').trigger('reset');
    $('#plateDetails').removeAttr("style").hide()
    $('#stolenMessage').removeAttr("style").hide()
  })

  function hideTicketCivPopover() {
    $('#ticket-civ-first-name').popover('hide')
    $('#ticket-civ-last-name').popover('hide')
  }
  function hideWarningCivPopover() {
    $('#warning-civ-first-name').popover('hide')
    $('#warning-civ-last-name').popover('hide')
  }

  function hideCivPopover() {
    $('#civ-first-name').popover('hide')
    $('#civ-last-name').popover('hide')
  }
</script>